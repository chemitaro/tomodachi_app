<h1>新規投稿</h1>
<%= form_with(model: @picture, local: true) do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
  <%= f.label :content%>
  <%= f.text_field :content %>
  </div>
  <div>
  <%= f.label :image %>
  <%= f.file_field :image %>
  </div>
  <%= f.submit "Create" %>


<% end %>